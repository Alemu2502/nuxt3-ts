{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext", 
    "module": "ESNext",
    "strict": true, 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true 
  },
  "include": [
    "nuxt.config.ts", 
    "types/**/*.ts",
    "pages/**/*.vue", 
    "components/**/*.vue" 
  ]
}
